      "Description": "This step open the browser"
    },
    {
      "Name": "NAVIGATE_URL",
      "Param": "#HelloTalent\\helloTalentWelcomePage.Url",
      "Description": "Navigate to"
    },
    {
      "Name": "CLICK_BUTTON",
      "Param": "#HelloTalent\\helloTalentWelcomePage.HelloTalentLogo",
      "Description": "Click on the logo"
    },
    {
      "Name": "CLICK_BUTTON",
      "Param": "#HelloTalent\\helloTalentWelcomePage.LoginMenuItem",
