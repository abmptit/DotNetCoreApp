      "Value": "$Password"
    },
    {
      "Name": "CLICK_BUTTON",
      "Param": "#HelloTalent\\helloTalentLoginPage.ConnectLogin",
      "Description": "Click on connect button"
    },
    {
      "Name": "ASSERT_TEXT",
      "Param": "#HelloTalent\\helloTalentProfilePage.UserMenuItemText",
      "value": "Ben Miled",
      "Description": "assert that the text exist and equal to the passed value"
    }
  ]
